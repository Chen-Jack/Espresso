{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
        "tslint-config-standard",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "ordered-imports": false,
        "member-access": false,
        "object-literal-sort-keys":false,
        "no-console": false,
        "forin":false,
        "variable-name": false,
        "no-empty": false,
        "no-shadowed-variable": false,
        "only-arrow-functions": false,
        
    },
    "rulesDirectory": []
}